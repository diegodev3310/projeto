{
  "name": "meu-chatbot",
  "version": "1.0.1",
  "description": "Chatbot com Puppeteer e WhatsApp Web.js",
  "main": "src/main.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js"
  },
  "dependencies": {
    "dotenv": "^17.0.1",
    "express": "^4.21.2",
    "googleapis": "^150.0.1",
    "pg": "^8.16.3",
    "puppeteer": "^24.10.2",
    "qrcode": "^1.5.4",
    "whatsapp-web.js": "^1.31.0"
  },
  "devDependencies": {
    "nodemon": "^3.1.9"
  },
  "build": {
    "appId": "com.seu.chatbot",
    "productName": "MeuChatBot",
    "directories": {
      "output": "dist"
    },
    "files": [
      "**/*"
    ],
    "win": {
      "target": "nsis",
      "icon": "assets/icon.ico"
    },
    "nsis": {
      "oneClick": false,
      "perMachine": true,
      "allowToChangeInstallationDirectory": true
    }
  },
  "keywords": [],
  "author": "",
  "license": "ISC"
}
